{
  "name": "openmeet",
  "version": "1.0.0",
  "description": "Open-Source Meeting Intelligence Platform - OpenMeet",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "start": "bash scripts/start-all.sh",
    "start:win": "scriptsstart-all.bat",
    "stop": "bash scripts/stop-all.sh",
    "status": "bash scripts/status.sh",
    "dev": "turbo run dev",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:web\" \"npm run dev:ai\"",
    "dev:web": "turbo run dev --filter=@openmeet/web",
    "dev:api": "turbo run dev --filter=@openmeet/api",
    "dev:ai": "turbo run dev --filter=@openmeet/ai-service",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:integration": "turbo run test:integration",
    "test:e2e": "turbo run test:e2e",
    "test:perf": "turbo run test:perf",
    "test:security": "npm audit && turbo run test:security",
    "security:scan": "ts-node infrastructure/security/security-scan.ts",
    "security:secrets": "ts-node infrastructure/security/secrets-scanner.ts",
    "security:pentest": "ts-node infrastructure/security/pen-test.ts",
    "security:verify": "./infrastructure/security/verify-security.sh",
    "security:all": "npm run security:scan && npm run security:secrets && npm run security:verify",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaospapagiannis/OpenMeet.git"
  },
  "keywords": [
    "meeting-intelligence",
    "transcription",
    "ai",
    "speech-to-text",
    "conversation-analytics",
    "open-source"
  ],
  "author": "OpenMeet Team",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "react-hook-form": "^7.64.0",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  }
}