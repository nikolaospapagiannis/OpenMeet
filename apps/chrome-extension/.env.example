# Chrome Extension Environment Configuration
# Copy this file to .env and update values for your environment

# API Configuration
API_URL=http://localhost:4100/api
WS_URL=ws://localhost:4100

# Web Frontend URL (for dashboard links)
WEB_URL=http://localhost:4200

# Extension Settings
EXTENSION_NAME=OpenMeet
EXTENSION_ENV=development

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_ERROR_REPORTING=true
ENABLE_AUTO_RECORD=true

# Production URLs (uncomment for production build)
# API_URL=https://api.yourapp.com/api
# WS_URL=wss://api.yourapp.com
# WEB_URL=https://app.yourapp.com
# EXTENSION_ENV=production
