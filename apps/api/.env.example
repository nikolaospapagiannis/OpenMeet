# Database Configuration
DATABASE_URL=postgresql://openmeet:openmeet123@localhost:5432/openmeet_db

# Zoom SDK Configuration (Required for Bot Integration)
# Obtain these from https://marketplace.zoom.us/
ZOOM_SDK_KEY=your_zoom_sdk_key_here
ZOOM_SDK_SECRET=your_zoom_sdk_secret_here

# Zoom OAuth App Configuration (Required for User Authentication)
# Create OAuth app at https://marketplace.zoom.us/develop/create
ZOOM_CLIENT_ID=your_zoom_oauth_client_id
ZOOM_CLIENT_SECRET=your_zoom_oauth_client_secret
ZOOM_WEBHOOK_SECRET=your_zoom_webhook_secret
ZOOM_ACCOUNT_ID=your_zoom_account_id

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS S3 Configuration (for recording storage)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your-recording-bucket

# Application Settings
NODE_ENV=development
PORT=3000
LOG_LEVEL=info
JWT_SECRET=your_jwt_secret_here